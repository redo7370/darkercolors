	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6b7280",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.sizeof",
				"keyword.operator.delete",
				"keyword.operator.typeof"
			],
			"settings": {
				"foreground": "#cccc00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted",
				"string.template",
				"string.interpolated"
			],
			"settings": {
				"foreground": "#22c55e"
			}
		},
		{
			"name": "String escape sequences",
			"scope": [
				"constant.character.escape",
				"string.regexp"
			],
			"settings": {
				"foreground": "#34d399"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.language.boolean",
				"constant.language.null",
				"constant.language.undefined"
			],
			"settings": {
				"foreground": "#06b6d4"
			}
		},
		{
			"name": "Class names - Warm Orange",
			"scope": [
				"entity.name.class",
				"entity.name.type.class",
				"support.class",
				"entity.other.inherited-class",
				"meta.cast entity.name.type",
				"storage.type.cs",
				"storage.type.java",
				"storage.type.cpp",
				"entity.name.type",
				"entity.name.type.template",
				"support.type",
				"variable.other.readwrite.member.cpp",
				"entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#f59e0b",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function names - Cool Blue",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call",
				"meta.method-call",
				"variable.function"
			],
			"settings": {
				"foreground": "#3b82f6"
			}
		},
		{
			"name": "Method names - Cool Blue",
			"scope": [
				"entity.name.function.member",
				"meta.method.java",
				"meta.method.cpp",
				"entity.name.function.method"
			],
			"settings": {
				"foreground": "#60a5fa"
			}
		},
		{
			"name": "Variable names",
			"scope": [
				"variable",
				"variable.other.readwrite",
				"variable.parameter",
				"meta.definition.variable"
			],
			"settings": {
				"foreground": "#e5e7eb"
			}
		},
		{
			"name": "Member variables and properties - Balanced Red",
			"scope": [
				"variable.other.property",
				"variable.other.member",
				"entity.name.variable.field"
			],
			"settings": {
				"foreground": "#ef4444"
			}
		},
		{
			"name": "Constants and enums",
			"scope": [
				"constant.language",
				"constant.other",
				"variable.other.constant",
				"entity.name.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#059669",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"punctuation.operator",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#ff8533"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.definition"
			],
			"settings": {
				"foreground": "#9ca3af"
			}
		},
		{
			"name": "Brackets and braces",
			"scope": [
				"punctuation.section.brackets",
				"punctuation.section.braces",
				"punctuation.section.parens",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly"
			],
			"settings": {
				"foreground": "#fbbf24"
			}
		},
		{
			"name": "Java/C++ specific types - Warm Orange (consistent with class names)",
			"scope": [
				"storage.type.primitive.java",
				"storage.type.built-in.primitive.cpp",
				"storage.type.primitive.cpp",
				"keyword.type.cs",
				"support.type.primitive",
				"storage.type.void",
				"keyword.type.void"
			],
			"settings": {
				"foreground": "#f59e0b",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Namespaces and packages",
			"scope": [
				"entity.name.namespace",
				"entity.name.package",
				"storage.modifier.package.java",
				"storage.modifier.import.java"
			],
			"settings": {
				"foreground": "#c77dff"
			}
		},
		{
			"name": "Imports and includes",
			"scope": [
				"keyword.control.import",
				"keyword.control.include",
				"meta.import",
				"meta.include"
			],
			"settings": {
				"foreground": "#ff1493"
			}
		},
		{
			"name": "Annotations and decorators",
			"scope": [
				"entity.name.function.decorator",
				"punctuation.decorator",
				"storage.type.annotation.java"
			],
			"settings": {
				"foreground": "#cccc00"
			}
		},
		{
			"name": "Labels",
			"scope": [
				"entity.name.label"
			],
			"settings": {
				"foreground": "#ff00ff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Preprocessor directives",
			"scope": [
				"meta.preprocessor",
				"keyword.control.directive",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#cccc00"
			}
		},
		{
			"name": "Template parameters",
			"scope": [
				"entity.name.type.template",
				"storage.type.template",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#ff9800"
			}
		},
		{
			"name": "Generic types - BRIGHT GOLD (consistent with class names)",
			"scope": [
				"entity.name.type.parameter",
				"storage.type.generic",
				"punctuation.definition.typeparameters"
			],
			"settings": {
				"foreground": "#ff9800",
				"fontStyle": "italic"
			}
		},
		{
			"name": "This/self keywords",
			"scope": [
				"variable.language.this",
				"keyword.other.this",
				"variable.language.self"
			],
			"settings": {
				"foreground": "#ff1493",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Access modifiers - GLOWING YELLOW (same as line numbers)",
			"scope": [
				"storage.modifier.access",
				"storage.modifier.java",
				"storage.modifier.cpp"
			],
			"settings": {
				"foreground": "#cccc00"
			}
		},
		{
			"name": "Exception handling",
			"scope": [
				"keyword.control.trycatch",
				"storage.type.java.exception"
			],
			"settings": {
				"foreground": "#ff3333"
			}
		},
		{
			"name": "Lambda expressions",
			"scope": [
				"storage.type.function.lambda",
				"keyword.operator.lambda"
			],
			"settings": {
				"foreground": "#ffa726"
			}
		}
	],
	"semanticTokenColors": {
		// Classes and Types - Warm Orange
		"class": "#f59e0b",
